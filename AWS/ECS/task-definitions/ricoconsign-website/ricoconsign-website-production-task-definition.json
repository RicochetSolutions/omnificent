{
  "family": "ricoconsign-website-production-task-definition",
  "volumes": [
    {
      "host": {
        "sourcePath": "/home/ec2-user/ricochet-web-settings/production/ricoconsign-website/blog/wp-config.php"
      },
      "name": "VolumeRicoconsignWebsiteBlog"
    },
    {
      "host": {
        "sourcePath": "/home/ec2-user/ricochet-web-settings/production/ricoconsign-website/store/wp-config.php"
      },
      "name": "VolumeRicoconsignWebsiteStore"
    },
    {
      "host": {
        "sourcePath": "/home/ec2-user/ricochet-web-settings/production/ricoconsign-website/support/wp-config.php"
      },
      "name": "VolumeRicoconsignWebsiteSupport"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "ricochetsolutions/ricoconsign-website-apache-php:latest",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 200,
      "name": "RicoconsignWebsiteApachePhp",
      "portMappings": [
        {
          "hostPort": 60002,
          "containerPort": 80
        },
        {
          "hostPort": 60003,
          "containerPort": 443
        }
      ],
      "volumesFrom": [
        {
          "sourceContainer": "RicoconsignWebsite"
        }
      ]
    },
    {
      "essential": true,
      "image": "ricochetsolutions/ricoconsign-website:%REVISION%",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "mountPoints": [
        {
          "sourceVolume": "VolumeRicoconsignWebsiteBlog",
          "containerPath": "/var/www/html/public/blog/wp-config.php"
        },
        {
          "sourceVolume": "VolumeRicoconsignWebsiteStore",
          "containerPath": "/var/www/html/public/store/wp-config.php"
        },
        {
          "sourceVolume": "VolumeRicoconsignWebsiteSupport",
          "containerPath": "/var/www/html/public/support/wp-config.php"
        }
      ],
      "name": "RicoconsignWebsite"
    }
  ]
}